{
  "name": "sentry-docs",
  "description": "Learn how to use Sentry, configure Sentry clients, and contribute to the open source project on GitHub",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getsentry/sentry-docs.git"
  },
  "author": "getsentry",
  "license": "BSL-1.1",
  "bugs": {
    "url": "https://github.com/getsentry/sentry-docs/issues"
  },
  "homepage": "https://docs.sentry.io/",
  "scripts": {
    "dev": "next-remote-watch ./src/platforms/**/*.mdx ./src/includes/**/*.mdx",
    "postinstall": "patch-package",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@github-docs/data-directory": "^1.2.0",
    "@sentry-internal/global-search": "^0.0.30",
    "@sentry/browser": "^5.22.3",
    "@sentry/gatsby": "^5.22.3",
    "@sentry/react": "^5.22.3",
    "@sentry/tracing": "^5.22.3",
    "@sentry/webpack-plugin": "^1.13.0",
    "algoliasearch": "^4.2.0",
    "axios": "^0.20.0",
    "bootstrap": "4.3.1",
    "copy-to-clipboard": "^3.3.1",
    "dompurify": "^2.0.12",
    "fs-extra": "^9.0.1",
    "github-slugger": "^1.3.0",
    "globby": "^11.0.1",
    "gray-matter": "^4.0.2",
    "js-yaml": "^3.14.0",
    "jsdom": "^16.3.0",
    "lodash.deburr": "^4.1.0",
    "mdast-util-to-string": "^2.0.0",
    "next": "^10.0.3",
    "next-mdx-remote": "^1.0.0",
    "platformicons": "^3.2.0",
    "prismjs": "^1.20.0",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.8",
    "react-helmet": "^5.2.1",
    "react-instantsearch-dom": "^6.4.0",
    "react-select": "^3.1.0",
    "remark-deflist": "^0.2.1",
    "remark-mdx": "^1.6.21",
    "remark-mdxjs": "^2.0.0-next.0",
    "remark-parse": "^9.0.0",
    "remark-stringify": "^8.1.0",
    "unified": "^9.2.0",
    "unist-util-visit": "^2.0.3"
  },
  "resolutions": {
    "@mdx-js/mdx": "2.0.0-next.8"
  },
  "devDependencies": {
    "@mdx-js/mdx": "2.0.0-next.8",
    "@testing-library/react": "^10.0.1",
    "@types/dompurify": "^2.0.3",
    "@types/fs-extra": "^9.0.4",
    "@types/jest": "^25.1.4",
    "@types/node": "12",
    "@types/react": "^16.9.27",
    "@types/react-select": "^3.0.19",
    "@types/unist": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-jest": "^25.2.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-mdx": "^1.8.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.3",
    "jest-watch-typeahead": "^0.5.0",
    "lint-staged": "^10.0.10",
    "next-remote-watch": "^0.2.0",
    "patch-package": "^6.2.2",
    "prettier": "^2.0.2",
    "sass": "^1.30.0",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7"
  },
  "volta": {
    "node": "12.18.3",
    "yarn": "1.22.4"
  }
}
